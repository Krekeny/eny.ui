{
  "name": "eny.ui",
  "version": "0.0.1",
  "license": "MIT",
  "main": "lib/module/index.js",
  "module": "lib/module/index.js",
  "react-native": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.json"
        }
      ]
    ],
    "exclude": "**/*.stories.*"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "lint": "eslint .",
    "generate": "node scripts/generate-index.js",
    "prepack": "npx bob build",
    "build": "npx bob build",
    "storybook": "cd storybook && npm run storybook",
    "storybook:android": "cd storybook && npm run storybook:android",
    "build-storybook": "cd storybook && npm run build-storybook"
  },
  "devDependencies": {
    "react-native-builder-bob": "^0.40.16",
    "typescript": "~5.9.2",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.73.0",
    "@storybook/react-native": "10.0.2"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "resolutions": {
    "react-docgen-typescript": "2.2.2",
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "overrides": {
    "react-docgen-typescript": "2.2.2"
  },
  "pnpm": {
    "overrides": {
      "react-docgen-typescript": "2.2.2"
    }
  },
  "packageManager": "yarn@4.12.0"
}
